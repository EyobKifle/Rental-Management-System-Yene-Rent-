{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!_next|api|assets|Css|Js|Html|favicon.ico|.*\\..*).*)",
      "destination": "/Html/$1.html"
    },
    {
      "source": "/:path((?!_next|api|assets|Css|Js|Html|favicon.ico).*)",
      "destination": "/Html/:path"
    },
    {
      "source": "/",
      "destination": "/Html/index.html"
    }
  ]
}
{
  "rewrites": [
    {
      "source": "/",
      "destination": "/Html/index.html"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/Html/:path.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=1, stale-while-revalidate=59"
        }
      ]
    }
  ]
}
{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(Css|Js|assets|lib)/(.*)",
      "destination": "/$1/$2"
    },
    {
      "source": "/((?!api/)[^.]*)",
      "destination": "/Html/$1.html"
    },
    {
      "source": "/",
      "destination": "/Html/index.html"
    }
  ],
  "cleanUrls": true
}
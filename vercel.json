{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/Html/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/Html/dashboard.html"
    },
    {
      "source": "/dashboard.html",
      "destination": "/Html/dashboard.html"
    },
    {
      "source": "/:path*",
      "destination": "/Html/:path*.html"
    },
    {
      "source": "/Css/:path*",
      "destination": "/Css/:path*"
    },
    {
      "source": "/Js/:path*",
      "destination": "/Js/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/images/:path*"
    }
  ]
}
{
  "version": 2,
  "rewrites": [
    {
      "source": "/:path(Css|Js|assets|lib)/:file",
      "destination": "/:path/:file"
    },
    {
      "source": "/:path*.html",
      "destination": "/Html/:path.html"
    },
    {
      "source": "/:path*",
      "destination": "/Html/:path*.html"
    }
  ]
}